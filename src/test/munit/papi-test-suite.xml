				</munit-tools:with-attributes>
				<munit-tools:before-call >
					<logger level="INFO" doc:name="Logger" doc:id="adbccd21-1a5e-4bd1-b8c0-ee3d328775cf" message="#[payload]"/>
					<munit-tools:assert doc:name="Assert expression" doc:id="7235864b-a7aa-4135-b866-470c9f55f023" />
				</munit-tools:before-call>
				<munit-tools:after-call >
					<logger level="INFO" doc:name="Logger" doc:id="0145a666-cacf-4cf5-9f0d-f55eb412a889" message="#[payload]"/>
					<munit-tools:assert-equals doc:name="Assert equals" doc:id="8c672e54-1e4b-4e43-8024-8b7505647c60" actual="#[payload]" expected='#[[&#10;    {&#10;        "hi": "yathendra"&#10;    }&#10;]]'/>
				</munit-tools:after-call>
			</munit-tools:spy>
		</munit:behavior>
		<munit:execution >
			<munit:set-event doc:name="Set Event" doc:id="2f62cb73-8069-4869-9472-edc838501133" >
				<munit:attributes value='#[readUrl("classpath://Munit/input.dwl")]' />
			</munit:set-event>
