	<munit:test name="papi-test-suite-papiFlowTest" doc:id="897cff1c-ff97-4a00-ac08-392d39a9e2d9" description="Test">
		<munit:behavior >
			<munit-tools:mock-when doc:name="Mock when" doc:id="ac88c068-4a94-437e-840b-94a054900be2" processor="http:request">
				<munit-tools:with-attributes >
